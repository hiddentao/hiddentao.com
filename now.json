{
  "version": 2,
  "name": "hiddentao-com-live",
  "alias": "hiddentao.com",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "public"
      }
    }
  ],
  "build": {
    "env": {
      "CONTENTFUL_ACCESS_TOKEN": "@hiddentao_contentful_access_token",
      "PRISMIC_ACCESS_TOKEN": "@hiddentao_prismic_access_token"
    }
  }
}